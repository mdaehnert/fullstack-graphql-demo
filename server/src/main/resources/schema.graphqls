type Query {
  getUser(id: Int!) : User
}

type Mutation {
  addBlogPost(newPost: BlogPostInput!) : BlogPost
}

input BlogPostInput {
  subject: String!
  content : String!
  userId: Int!
}


type User {
  id: Int!
  name: String!
  birthdate: String
}

type BlogPost {
  id: Int!
  createdAt: String
  subject: String!
  content: String!
}
